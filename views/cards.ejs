<div class="card">
  <a href="/detalhes/<%= filme.id %>"><img id="img" src="<%= filme.imagem %>" alt=""></a>
  <div id="descricao">
    <h3><%= filme.nome %></h3>
    <p><%= filme.descricao %></p>
  </div>
  <div id="editar">
    <a href="/getById/<%= filme.id %>/put">Editar</a>
  </div>
  <div id="apagar">
    <a href="/getById/<%= filme.id %>/delete">Apagar</a>
  </div>
</div>
